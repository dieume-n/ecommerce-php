{
    "description": "Ecommerce website with php mvc workflow from scratch",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "vlucas/phpdotenv": "^4.1",
        "altorouter/altorouter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
